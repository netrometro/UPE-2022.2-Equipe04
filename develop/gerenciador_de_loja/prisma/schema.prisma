generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model produtos {
  id         Int      @id @default(autoincrement())
  tipo       String
  tamanho    String
  cor        String
  material   String?
  marca      String
  quantidade Int
  preco      Float
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
